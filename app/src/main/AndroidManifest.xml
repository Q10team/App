<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamproject">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TeamProject">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.LoginActivity"></activity>
        <activity android:name=".login.RegisterActivity"></activity>
        <activity android:name=".TodoList.MainActivity"></activity>
        <activity android:name=".TodoList.ListDetail"></activity>
        <activity android:name=".TodoList.MakeTodoList"></activity>
        <activity android:name=".TodoList.UpdateTodoList"></activity>
    </application>

</manifest>