<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_td"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_td_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TeamProject"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".front.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.TeamProject.NoActionBar"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.LoginActivity" />
        <activity android:name=".login.RegisterActivity" />
        <activity android:name=".TodoList.MainActivity" />
        <activity android:name=".TodoList.ListDetail" />
        <activity android:name=".TodoList.MakeTodoList" />
        <activity android:name=".TodoList.UpdateTodoList" />
    </application>

</manifest>